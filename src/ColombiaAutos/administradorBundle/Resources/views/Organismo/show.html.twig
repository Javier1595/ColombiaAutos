{% extends '::base.html.twig' %}

{% block body -%}
<div class="container">
    <ul class="record_actions">
        <li>
            <a href="{{ path('organismo') }}">
                Volver
            </a>
        </li>
    <!--
        <li>
            <a href="{{ path('organismo_edit', { 'id': entity.codOrganismo }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>-->
    </ul>
    
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Codigo organismo</th>
                <td>{{ entity.codOrganismo }}</td>
            </tr>
            <tr>
                <th>Nombre organismo</th>
                <td>{{ entity.nombreOrganismo }}</td>
            </tr>
            <tr>
                <th>Telefono organismo</th>
                <td>{{ entity.telefonoOrganismo }}</td>
            </tr>
        </tbody>
    </table>
    <table class="records_list">
        <thead>
            <tr>
                <th>Codigo ciudad</th>
                <th>Nombre ciudad</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
        {% for ciu in entities %}
            <tr>
                <td><!--<a href="{{ path('organismo_show', { 'id': ciu.codCiudad }) }}">{{ ciu.codCiudad }}</a>-->{{ ciu.codCiudad }}</td>
                <td>{{ ciu.nombreCiudad }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}