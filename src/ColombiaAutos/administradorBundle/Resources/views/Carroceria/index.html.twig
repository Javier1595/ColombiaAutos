{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Carroceria list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Codcarroceria</th>
                <th>Tipocarroceria</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('carroceria_show', { 'id': entity.codCarroceria }) }}">{{ entity.codCarroceria }}</a></td>
                <td>{{ entity.tipoCarroceria }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('carroceria_show', { 'id': entity.codCarroceria }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('carroceria_edit', { 'id': entity.codCarroceria }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('carroceria_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
