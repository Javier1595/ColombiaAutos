<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Motor" table="motor">
    <id name="nroMotor" type="string" column="nro_motor" length="3">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cilindraje" type="string" column="cilindraje" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="combustible" type="string" column="combustible" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="potencia" type="string" column="potencia" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="placa" target-entity="Vehiculo" mapped-by="nroMotor" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
