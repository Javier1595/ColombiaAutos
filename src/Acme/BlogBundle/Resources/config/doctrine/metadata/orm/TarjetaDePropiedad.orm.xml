<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TarjetaDePropiedad" table="tarjeta_de_propiedad">
    <indexes>
      <index name="vehiculo_tarjeta_de_propiedad" columns="placa"/>
      <index name="organismo_tarjeta_de_propiedad" columns="cod_organismo"/>
    </indexes>
    <id name="nroLicencia" type="string" column="nro_licencia" length="11">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechaExpedicion" type="date" column="fecha_expedicion" nullable="false"/>
    <field name="fechaMatricula" type="date" column="fecha_matricula" nullable="false"/>
    <field name="fechaVencimiento" type="date" column="fecha_vencimiento" nullable="true"/>
    <many-to-one field="codOrganismo" target-entity="Organismo" fetch="LAZY">
      <join-columns>
        <join-column name="cod_organismo" referenced-column-name="cod_organismo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="placa" target-entity="Vehiculo" fetch="LAZY">
      <join-columns>
        <join-column name="placa" referenced-column-name="placa"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="nroentificacion" target-entity="Propietario" inversed-by="nroLicencia" fetch="LAZY">
      <join-table name="tarjeta_propietario">
        <join-columns>
          <join-column name="nro_licencia" referenced-column-name="nro_licencia"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="nro_identificacion" referenced-column-name="nro_identificacion"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
