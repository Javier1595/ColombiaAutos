<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VehiculoVehiculopesado" table="vehiculo_vehiculopesado">
    <id name="placa" association-key="true"/>
    <field name="capacidad" type="string" column="capacidad" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nroEjes" type="string" column="nro_ejes" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="placa" target-entity="Vehiculo" fetch="LAZY">
      <join-columns>
        <join-column name="placa" referenced-column-name="placa"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
